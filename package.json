{
  "name": "survivejs-training",
  "private": true,
  "version": "0.0.0",
  "description": "SurviveJS - Training material",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "NODE_ENV=production webpack --config webpack.config.production.js",
    "lint": "eslint --ext .js,.jsx .",
    "deploy": "npm run build && npm run copy && node ./src/deploy.js",
